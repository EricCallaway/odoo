<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <!--Action for the patient menu-->
    <record id="action_estate_property" model="ir.actions.act_window">
            <field name="name">Real Estate Property Action</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">estate.property</field>
            <field name="view_type">form</field>
            <field name="view_mode">form,tree</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create First Property Listing!
                </p>
            </field>
        </record>

    <menuitem id="real_estate_root" name="Real Estate" sequence="0" web_icon="eric_real_estate/static/description/icon.png"/>
    <menuitem id="property" name="Property" parent="real_estate_root" action="action_estate_property"/>
</odoo>